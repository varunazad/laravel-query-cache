{
    "name": "varunazad/laravel-query-cache",
    "description": "Automatic query caching for Laravel with intelligent invalidation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Varun Chaturvedi",
            "email": "varunindian10@gmail.com",
            "homepage": "https://github.com/varunazad"
        }
    ],
    "keywords": [
        "laravel",
        "query cache",
        "performance",
        "eloquent"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/cache": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Varunazad\\QueryCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Varunazad\\QueryCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Varunazad\\QueryCache\\QueryCacheServiceProvider"
            ],
            "aliases": {
                "QueryCache": "Varunazad\\QueryCache\\Facades\\QueryCache"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
